datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model News {
  id        Int      @id @default(autoincrement())
  title String?
  content String?
  url String?
  publisher String?
  publishedAt DateTime?
  writer String?
  category String
}
